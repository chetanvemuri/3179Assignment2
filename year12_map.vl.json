{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "VCE (Year 12) Certification by State/Territory",
  "width": 900,
  "height": 560,
  "projection": {
    "type": "mercator"
  },
  "data": {
    "url": "au_admin_1_states_provinces.json",
    "format": {
      "type": "topojson",
      "feature": "ne_10m_admin_1_states_provinces"
    }
  },
  "transform": [
    {
      "calculate": "datum.properties && datum.properties.name ? datum.properties.name : null",
      "as": "region_name"
    },
    {
      "filter": "isValid(datum.region_name)"
    },
    {
      "lookup": "region_name",
      "from": {
        "data": {
          "url": "year12_certification_by_state.csv"
        },
        "key": "STE_NAME21",
        "fields": [
          "Year12_rate"
        ]
      },
      "default": null
    },
    {
      "calculate": "toNumber(datum.Year12_rate)",
      "as": "Year12_rate_num"
    }
  ],
  "mark": {
    "type": "geoshape",
    "stroke": "#ffffff",
    "strokeWidth": 0.5
  },
  "encoding": {
    "color": {
      "field": "Year12_rate_num",
      "type": "quantitative",
      "title": "Year 12 certification (%)",
      "scale": {
        "scheme": "blues"
      },
      "legend": {
        "format": ".0f"
      }
    },
    "tooltip": [
      {
        "field": "region_name",
        "title": "State/Territory"
      },
      {
        "field": "Year12_rate_num",
        "title": "Year 12 rate (%)",
        "format": ".1f"
      }
    ]
  },
  "config": {
    "view": {
      "stroke": null
    },
    "background": "transparent"
  }
}
